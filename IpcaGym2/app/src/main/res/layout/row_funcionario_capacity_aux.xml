                <vough.example.ipcagym.views.VerticalBarCapacityView
                    android:id="@+id/capacitySaturday"
                    android:layout_width="20dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="16dp"
