                    android:layout_width="50dp"
                    android:layout_height="20dp"
                    android:layout_marginStart="8dp"
                    android:background="#00FFFFFF"
                    android:padding="0dp"
                    android:text="Friday"
                    android:textColor="#FFFFFF"
                    android:textSize="8sp"
